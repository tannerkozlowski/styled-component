{
  "name": "styled-components-docs",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "deploy": "now -t $NOW_TOKEN -T styledcomponents && now alias -t $NOW_TOKEN -T styledcomponents",
    "lint:js": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:css": "stylelint './static/**/*.js' './pages/**/*.js' './test/**/*.js' './utils/**/*.js' './components/**/*.js' './sections/**/*.js'",
    "lint": "run-s lint:js lint:css",
    "start": "NODE_ENV=production node server.js",
    "test": "jest -c .jest.config.js",
    "preintegration": "next build",
    "integration": "jest -c .jest-integration.config.js --forceExit --silent --runInBand",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-styled-components": "^1.6.0",
    "commonmark": "^0.28.1",
    "commonmark-react-renderer": "^4.3.4",
    "express": "^4.16.3",
    "invariant": "^2.2.4",
    "isomorphic-fetch": "^2.2.1",
    "lru-cache": "^4.1.3",
    "module-alias": "^2.1.0",
    "next": "^4.0.0",
    "polished": "^2.0.3",
    "preact": "^8.3.1",
    "preact-compat": "^3.18.4",
    "prismjs": "^1.6.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-live": "^1.8.0-2",
    "react-octicons-svg": "^1.1.13",
    "styled-components": "^3.4.5",
    "styled-theming": "^2.2.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "devDependencies": {
    "babel-core": "^6",
    "babel-eslint": "^9.0.0",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.5.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "glob": "^7.1.3",
    "glob-promise": "^3.4.0",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "jest-image-snapshot": "^2.5.0",
    "jest-styled-components": "^6.1.1",
    "lint-staged": "^7.2.2",
    "now": "^11.4.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.5.3",
    "puppeteer": "^1.7.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.4.2",
    "stylelint": "^8.0.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-processor-styled-components": "^0.2.1",
    "stylint-stylish": "^1.4.1"
  },
  "lint-staged": {
    "*.js": [
      "jest -c .jest.config.js --findRelatedTests",
      "stylelint",
      "eslint --fix",
      "git add"
    ]
  }
}
